{
    "manifest_version": 2,
    "name": "RelaxMode",
    "description": "RelaxMode - Play relaxing music whilst browsing",
    "version": "1.0.0",
    "browser_action": {
        "default-title" : "RelaxMode",
        "default_popup" : "popup.html"
    },
    "background": {
        "page" : "background.html",
        "persistent" : true
    }

}